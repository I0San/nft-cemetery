{
  "name": "nft-cemetery-dapp",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "connectkit": "^1.4.0",
    "env-cmd": "^10.1.0",
    "framer-motion": "^10.12.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "use-debounce": "^9.0.4",
    "viem": "^1.2.15",
    "wagmi": "^1.3.8",
    "zustand": "^4.3.9"
  },
  "scripts": {
    "dev": "yarn dev-hardhat",
    "dev-hardhat": "env-cmd -f ./.env.hardhat react-scripts start",
    "dev-testnet": "env-cmd -f ./.env.testnet react-scripts start",
    "dev-mainnet": "env-cmd -f ./.env.mainnet react-scripts start",
    "build": "yarn build-hardhat",
    "build-hardhat": "env-cmd -f ./.env.hardhat react-scripts build",
    "build-testnet": "env-cmd -f ./.env.testnet react-scripts build",
    "build-mainnet": "env-cmd -f ./.env.mainnet react-scripts build",
    "start": "yarn start-hardhat",
    "start-hardhat": "env-cmd -f ./.env.hardhat react-scripts start",
    "start-testnet": "env-cmd -f ./.env.testnet react-scripts start",
    "start-mainnet": "env-cmd -f ./.env.mainnet react-scripts start"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie <= 99",
      "not android <= 4.4.4",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
